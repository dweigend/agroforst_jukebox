{
  "name": "agroforst-visualizer",
  "version": "1.0.0",
  "description": "Interactive 3D Music Visualizer for Agroforestry Systems with RFID Integration",
  "author": "David Weigend <david@weigend.studio>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dweigend/agroforst_jukebox.git"
  },
  "homepage": "https://github.com/dweigend/agroforst_jukebox#readme",
  "bugs": {
    "url": "https://github.com/dweigend/agroforst_jukebox/issues"
  },
  "keywords": [
    "agroforestry",
    "3d-visualization",
    "threejs",
    "rfid",
    "music-visualizer",
    "interactive-installation"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "lint:fix": "eslint 'src/**/*.{ts,js}' --fix",
    "format": "prettier --write src/**/*.{ts,js,json,md}",
    "format:check": "prettier --check src/**/*.{ts,js,json,md}",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "clean": "rm -rf dist node_modules",
    "reinstall": "npm run clean && npm install"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/eslint__js": "^8.42.3",
    "@types/three": "^0.179.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.41.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "beercss": "^3.11.33",
    "howler": "^2.2.4",
    "material-dynamic-colors": "^1.1.2",
    "simplex-noise": "^4.0.3",
    "three": "^0.179.1"
  }
}
